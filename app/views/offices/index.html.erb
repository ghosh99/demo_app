<h2>All Listings</h2>
<div class="row">
    <div id="offices" class="span12">

    <% @offices.each do |office| %>

      <div class="row search_result">

        <div class="span2">
            <a href= "<%= office_path(office)  %>" >
                <%= image_tag office.image1_url(:small).to_s, :class=> "img-polaroid", :size=> "114x74", :alt=> "Photo" %>
            </a>
        </div>

        <div class="span6">
            <%= link_to office.title, office %>
        </div>

        <div class="span2">
          <%= office.loc_city %> <%= office.loc_state %>
        </div>



        <div class="span2">
            $ <%= office.rent %>
        </div>

      </div>

    <% end %>
    </div>
  <br>
</div>